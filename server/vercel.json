{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "../index.html",
        "use": "@vercel/static"
      },
      {
        "src": "../css/**/*.css",
        "use": "@vercel/static"
      },
      {
        "src": "../js/**/*.js",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/css/(.*)",
        "dest": "/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/js/$1"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  